{
  "name": "templet-cli-plus",
  "version": "0.0.7",
  "description": "一个融合了多种指令的脚手架工具，都是我平时用到的，所以就把它们整合到一起了，方便自己使用，也方便大家使用。",
  "scripts": {
    "dev": "NODE_ENV=development ENV=daily rollup -c -w",
    "cli": "node ./bin/index.cjs.js",
    "build": "NODE_ENV=production ENV=prod rollup -c",
    "prepare": "husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint-staged": "lint-staged",
    "commitlint": "commitlint --edit",
    "test:commitlint": "node test-commitlint.js"
  },
  "bin": {
    "temp": "./bin/index.cjs.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xcy960815/templet-cli-plus.git"
  },
